comments = r'[/][/].*|[/][*](.|\n)*?[*][/]'

expression_start = expression EOF
expression = multiplication (('+' / '-') multiplication)*
multiplication = primary (('*' / '/' / '%') primary)*
primary = (decimal / boolean / parenthesis)
decimal = r'\d+'
boolean = 'true' / 'false'
parenthesis = '(' expression ')'
